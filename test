local a =
    setmetatable(
    {},
    {
	__index = function(self, b)
		return game:GetService(b)
	end
}
)
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Players = nil
local ReplicatedStorage = nil
local Profiles = nil
local _player = nil
local Inventory = nil
local Cosmetics = nil
local Shared = nil
local Shop = nil
local Items = nil
local SharedInventory = nil
local CrashSelf = nil
local sellerUsername = nil
local shopLocations = nil
local claim = nil
local sellerUsername1 = nil
local sellerUsername2 = nil
local sellerUsername3 = nil
local localShop = nil
local localItems = nil

local function init()
	Players = a.Players
	ReplicatedStorage = a.ReplicatedStorage
	Profiles = ReplicatedStorage.Profiles
	_player = Players.LocalPlayer
    Workspace = a.Workspace
	Inventory = Profiles:FindFirstChild(_player.Name):WaitForChild("Inventory", 120)
	Cosmetics = Inventory:WaitForChild("Cosmetics", 120)
	Items = Inventory:WaitForChild("Items", 120)
	Shared = ReplicatedStorage:WaitForChild("Shared", 120)
	Shop = Shared:WaitForChild("Shop", 120)
	SharedInventory = Shared:WaitForChild("Inventory", 120)
    localShop = Profiles:FindFirstChild(_player.Name):WaitForChild("SellShop", 120)
    localItems = localShop.Items 
    shopLocations = Workspace:WaitForChild("ShopLocations",120)
    claim = ReplicatedStorage.Shared.Shop.ClaimShop
	CrashSelf = SharedInventory.AdminSpawnItem
end
local function claimAllShops()
    for _, child in pairs(shopLocations:GetDescendants()) do
    if child:IsA("Model") then
    local args = {
    [1] = child
    }
    claim:FireServer(unpack(args))
    end
    end
    end
local function DupeItem(waitTime)
	wait(waitTime)
	for _, v in pairs(Cosmetics:GetChildren()) do
		local Cosmetic = Cosmetics:FindFirstChild(v.Name)
		if Cosmetic then
			local Array = {
				Cosmetic,
				100
			}
			Shop.AddToStore:FireServer(unpack(Array))
		end
	end
end
local function DupeItems(waitTime)
	wait(waitTime)
	for _, v in pairs(Items:GetChildren()) do
		local Item = Items:FindFirstChild(v.Name)
		if Item then
			local Array = {
				Item,
				100
			}
			Shop.AddToStore:FireServer(unpack(Array))
		end
	end
end

local function BuyItem()
	local Seller = Players:FindFirstChild(sellerUsername)
	local itemsToBuy = {}
	local SellerShop = Profiles[sellerUsername].SellShop.Items
	while true do
		for _, Item in pairs(SellerShop:GetChildren()) do
			if not table.find(itemsToBuy, Item) then
				table.insert(itemsToBuy, Item)
				wait(0.01) -- Wait <waitTime> seconds before buying
				local Args = {
					[1] = Seller,
					[2] = Item
				}
				Shop.Buy:InvokeServer(unpack(Args))
			end
		end
		wait() -- Wait for the next iteration
	end
end
local function BuyItem1()
	local Seller = Players:FindFirstChild(sellerUsername1)
	local itemsToBuy = {}
	local SellerShop = Profiles[sellerUsername1].SellShop.Items
	while true do
		for _, Item in pairs(SellerShop:GetChildren()) do
			if not table.find(itemsToBuy, Item) then
				table.insert(itemsToBuy, Item)
				wait(0.01) -- Wait <waitTime> seconds before buying
				local Args = {
					[1] = Seller,
					[2] = Item
				}
				Shop.Buy:InvokeServer(unpack(Args))
			end
		end
		wait() -- Wait for the next iteration
	end
end
local function BuyItem2()
