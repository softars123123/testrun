local function BuyItem3()
	local Seller = Players:FindFirstChild(sellerUsername3)
	local itemsToBuy = {}
	local SellerShop = Profiles[sellerUsername3].SellShop.Items
	while true do
		for _, Item in pairs(SellerShop:GetChildren()) do
			if not table.find(itemsToBuy, Item) then
				table.insert(itemsToBuy, Item)
