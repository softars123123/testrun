local a =
    setmetatable(
    {},
    {
	__index = function(self, b)
		return game:GetService(b)
	end
}
)
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Players = nil
local ReplicatedStorage = nil
local Profiles = nil
local _player = nil


local function init()
	Players = a.Players
	ReplicatedStorage = a.ReplicatedStorage
	Profiles = ReplicatedStorage.Profiles
	_player = Players.LocalPlayer
    Workspace = a.Workspace
end


local Window = Rayfield:CreateWindow({
	Name = "G-Hub",
	LoadingTitle = "G-Hub V0.1",
	LoadingSubtitle = "by Glory",
	ConfigurationSaving = {
		Enabled = false,
		FolderName = "G-Hub", -- Create a custom folder for your hub/game
		FileName = "G-Hub"
	},
	Discord = {
		Enabled = true,
		Invite = "https://discord.gg/k5M6bktKhq", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD
		RememberJoins = true -- Set this to false to make them join the discord every time they load it up
	},
	KeySystem = false, -- Set this to true to use our key system
	KeySettings = {
		Title = "Untitled",
		Subtitle = "Key System",
		Note = "No method of obtaining the key is provided",
		FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
		SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
		GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
		Key = {
			"Hello"
		} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
	}
})

local AutoFarmTab = Window:CreateTab("Auto-Farm", nil) -- Title, Image
local MainSection = AutoFarmTab:CreateSection("Auto-Farm")

local DupeButton = AutoFarmTab:CreateButton({
	Name = "Dupe cosmetic and Disconnect",
	Callback = function()
		CrashSelf:FireServer()
		DupeItem(DupeTime)
	end,
})

local DupeButton1 = AutoFarmTab:CreateButton({
	Name = "Dupe item and Disconnect",
	Callback = function()
		CrashSelf:FireServer()
		DupeItems(DupeTime)
	end,
})

local SetShop = AutoFarmTab:CreateButton({
	Name = "Set Shop",
	Callback = function()
        removeAllItemsFromStore()
		claimAllShops()
	end,
})
local BuyTab = Window:CreateTab("Buyer", nil) -- Title, Image
local BuySection = BuyTab:CreateSection("Buyer")
local BuyInput = BuyTab:CreateInput({
	Name = "Seller Username",
	PlaceholderText = "Input Seller 1st Username",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
		sellerUsername = Text
	end,
})
local BuyInput = BuyTab:CreateInput({
	Name = "Seller Username",
	PlaceholderText = "Input Seller 2nd Username",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
		sellerUsername1 = Text
	end,
})
local BuyInput = BuyTab:CreateInput({
	Name = "Seller Username",
	PlaceholderText = "Input Seller 3rd Username",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
		sellerUsername2 = Text
	end,
})
local BuyInput = BuyTab:CreateInput({
	Name = "Seller Username",
	PlaceholderText = "Input Seller 4th Username",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
		sellerUsername3 = Text
	end,
})
local BuyButton = BuyTab:CreateButton({
    Name = "Start Auto Buy",
    Callback = function()
    Rayfield:Notify({
    Title = "Started Auto buy",
    Content = "Auto buy Started",
    Duration = 3,
    Image = nil,
    Actions = nil,
    })
    
    spawn(function()
    BuyItem()
    end)
    
    spawn(function()
    BuyItem1()
    end)
    
    spawn(function()
    BuyItem2()
    end)
    
    spawn(function()
    BuyItem3()
    end)
    end,
    })
local GoldDupeTime = 1.1
local GoldDupeTab = Window:CreateTab("Gold Dupe", nil) -- Title, Image
local GoldDupeSection = GoldDupeTab:CreateSection("Gold Dupe")
local GoldDupedFromInput = GoldDupeTab:CreateInput({
	Name = "Seller Username",
	PlaceholderText = "Input Seller Username",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
		sellerUsername = Text
	end,
})
local GoldDupeInput = GoldDupeTab:CreateInput({
	Name = "Wait Time",
	PlaceholderText = "1.1",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
		GoldDupeTime = tonumber(Text)
	end,
})
local GoldDupeButton = GoldDupeTab:CreateButton({
	Name = "Gold Dupe and Disconnect",
	Callback = function()
		CrashSelf:FireServer()
		wait(GoldDupeTime)
		BuyItem()
	end,
})


init()
