				local Args = {
					[1] = Seller,
					[2] = Item
				}
				Shop.Buy:InvokeServer(unpack(Args))
			end
		end
		wait() -- Wait for the next iteration
	end
end
local function removeAllItemsFromStore()
    local args = {}
    for _, item in pairs(localItems:GetChildren()) do
    args[#args + 1] = item
    end
    for _, item in pairs(args) do
    Shop.RemoveFromStore:FireServer(item)
    wait(0.01)
    end
    end


local Window = Rayfield:CreateWindow({
	Name = "Bendsai",
	LoadingTitle = "Bendsai 0.01",
	LoadingSubtitle = "by 3m kensai",
	ConfigurationSaving = {
		Enabled = false,
		FolderName = "sup Bro", -- Create a custom folder for your hub/game
		FileName = "Big Hub"
	},
	Discord = {
		Enabled = false,
		Invite = "noinvitelink", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD
		RememberJoins = true -- Set this to false to make them join the discord every time they load it up
	},
	KeySystem = false, -- Set this to true to use our key system
	KeySettings = {
		Title = "Untitled",
		Subtitle = "Key System",
		Note = "No method of obtaining the key is provided",
		FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
		SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
		GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
		Key = {
			"Hello"
		} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
	}
})

local MainTab = Window:CreateTab("Duper", nil) -- Title, Image
local MainSection = MainTab:CreateSection("Duper")
local DupeTime = 0.5
local MainInput = MainTab:CreateInput({
	Name = "Wait Time",
	PlaceholderText = "0.5",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
		DupeTime = tonumber(Text)
	end,
})
local DupeButton = MainTab:CreateButton({
	Name = "Dupe cosmetic and Disconnect",
	Callback = function()
		CrashSelf:FireServer()
		DupeItem(DupeTime)
	end,
})

local DupeButton1 = MainTab:CreateButton({
	Name = "Dupe item and Disconnect",
	Callback = function()
		CrashSelf:FireServer()
		DupeItems(DupeTime)
	end,
})

local SetShop = MainTab:CreateButton({
	Name = "Set Shop",
	Callback = function()
        removeAllItemsFromStore()
		claimAllShops()
	end,
})
local BuyTab = Window:CreateTab("Buyer", nil) -- Title, Image
local BuySection = BuyTab:CreateSection("Buyer")
local BuyInput = BuyTab:CreateInput({
	Name = "Seller Username",
	PlaceholderText = "Input Seller 1st Username",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
		sellerUsername = Text
	end,
})
local BuyInput = BuyTab:CreateInput({
	Name = "Seller Username",
	PlaceholderText = "Input Seller 2nd Username",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
		sellerUsername1 = Text
	end,
})
local BuyInput = BuyTab:CreateInput({
	Name = "Seller Username",
	PlaceholderText = "Input Seller 3rd Username",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
		sellerUsername2 = Text
	end,
})
local BuyInput = BuyTab:CreateInput({
	Name = "Seller Username",
	PlaceholderText = "Input Seller 4th Username",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
		sellerUsername3 = Text
	end,
})
local BuyButton = BuyTab:CreateButton({
    Name = "Start Auto Buy",
    Callback = function()
    Rayfield:Notify({
    Title = "Started Auto buy",
    Content = "Auto buy Started",
    Duration = 3,
    Image = nil,
    Actions = nil,
    })
    
    spawn(function()
    BuyItem()
    end)
    
    spawn(function()
    BuyItem1()
    end)
    
    spawn(function()
    BuyItem2()
    end)
    
    spawn(function()
    BuyItem3()
    end)
    end,
    })
local GoldDupeTime = 1.1
local GoldDupeTab = Window:CreateTab("Gold Dupe", nil) -- Title, Image
local GoldDupeSection = GoldDupeTab:CreateSection("Gold Dupe")
local GoldDupedFromInput = GoldDupeTab:CreateInput({
	Name = "Seller Username",
	PlaceholderText = "Input Seller Username",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
		sellerUsername = Text
	end,
})
local GoldDupeInput = GoldDupeTab:CreateInput({
	Name = "Wait Time",
	PlaceholderText = "1.1",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
		GoldDupeTime = tonumber(Text)
	end,
})
local GoldDupeButton = GoldDupeTab:CreateButton({
	Name = "Gold Dupe and Disconnect",
	Callback = function()
		CrashSelf:FireServer()
		wait(GoldDupeTime)
		BuyItem()
	end,
})
local BindsTab = Window:CreateTab("Binds", nil) -- Title, Image
local BindsSection = BindsTab:CreateSection("Binds")
local Keybind = BindsTab:CreateKeybind({
	Name = "claim shop",
	CurrentKeybind = "Y",
	HoldToInteract = false,
	Callback = function(Keybind)
        removeAllItemsFromStore()
        claimAllShops()
	end,
})
local Keybind = BindsTab:CreateKeybind({
	Name = "Dupe Cosmetics",
	CurrentKeybind = "X",
	HoldToInteract = false,
	Callback = function(Keybind)
        CrashSelf:FireServer()
        DupeItem(DupeTime)
	end,
})
local Keybind = BindsTab:CreateKeybind({
	Name = "Dupe Items",
	CurrentKeybind = "J",
	HoldToInteract = false,
	Callback = function(Keybind)
        CrashSelf:FireServer()
        DupeItems(DupeTime)
	end,
})
local Keybind = BindsTab:CreateKeybind({
	Name = "Increase wait by 0.01",
	CurrentKeybind = "F",
	HoldToInteract = false,
	Callback = function(Keybind)
        DupeTime = DupeTime + 0.01
        Rayfield:Notify({
			Title = "Adjust Dupe Time",
			Content = tostring(DupeTime),
			Duration = 1,
			Image = nil,
			Actions = nil,
		})
	end,
})
local Keybind = BindsTab:CreateKeybind({
	Name = "Decrease wait by 0.01",
	CurrentKeybind = "G",
	HoldToInteract = false,
	Callback = function(Keybind)
        DupeTime = DupeTime - 0.01
        Rayfield:Notify({
			Title = "Adjust Dupe Time",
			Content = tostring(DupeTime),
			Duration = 1,
			Image = nil,
			Actions = nil,
		})
	end,
})

init()
